= form_for @venue_picture do |f|
  - if @venue_picture.errors.any?
    #error_explanation
      %h2= "#{pluralize(@venue_picture.errors.count, "error")} prohibited this venue_picture from being saved:"
      %ul
        - @venue_picture.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :venue_id
    = f.number_field :venue_id
  .field
    = f.label :avatar
    = f.text_field :avatar
  .actions
    = f.submit 'Save'
